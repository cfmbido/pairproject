<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <table>
        <thead>
            <tr>
                <th>Name</th>
                <th>Gender</th>
                <th>Email</th>
                <th>Phone Number</th>
                <th>Role</th>
            </tr>
        </thead>
        <tbody>
            <% for (let i = 0; i < data.length; i++) { %>
            <tr>
                <td><%= data[i].first_name + ' ' + data[i].last_name %></td>
                <td><%= data[i].gender %></td>
                <td><%= data[i].email %></td>
                <td><%= data[i].phone_number %></td>
                <td><%= data[i].role %></td>
            </tr>
            <% } %>
        </tbody>
    </table>
</body>
</html>